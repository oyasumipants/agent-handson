# AWS認証情報
# AWSアクセスキーIDを設定してください
AWS_ACCESS_KEY_ID=your_access_key_id_here

# AWSシークレットアクセスキーを設定してください
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# AWSリージョン（デフォルト: us-west-2）
AWS_DEFAULT_REGION=us-west-2

# Tavily認証情報
# Tavily APIキーを設定してください（https://tavily.com/）
TAVILY_API_KEY=your_tavily_api_key_here

# Bedrock AgentCore設定
# Bedrock Agent RuntimeのARNを設定してください
# 例: arn:aws:bedrock-agentcore:us-west-2:123456789012:runtime/your-agent-name
AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:us-west-2:YOUR_ACCOUNT_ID:runtime/YOUR_AGENT_NAME

# Langfuse認証情報（オプション）
# Langfuseシークレットキーを設定してください
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here

# Langfuseパブリックキーを設定してください
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here

# LangfuseホストURL
LANGFUSE_HOST=https://us.cloud.langfuse.com
